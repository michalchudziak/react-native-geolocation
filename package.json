{
  "name": "@react-native-community/geolocation",
  "version": "3.0.0-rc.0",
  "description": "React Native Geolocation Module for iOS and Android",
  "react-native": "js/index",
  "main": "lib/commonjs/index",
  "source": "js/index",
  "files": [
    "lib/",
    "js/",
    "typings/",
    "/android",
    "!/android/build",
    "/ios",
    "/*.podspec"
  ],
  "typings": "typings/index.d.ts",
  "author": "React Native Community",
  "contributors": [
    "Michal Chudziak <michal.chudziak@gmail.com>"
  ],
  "homepage": "https://github.com/michalchudziak/react-native-geolocation#README.md",
  "license": "MIT",
  "scripts": {
    "start": "react-native start",
    "test": "yarn validate:eslint && yarn validate:flow && yarn test:jest",
    "validate:eslint": "eslint 'js/**/*.js' 'example/**/*.js'",
    "validate:flow": "flow check",
    "test:jest": "jest js/",
    "prepare": "bob build"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/michalchudziak/react-native-geolocation.git"
  },
  "keywords": [
    "react-native",
    "geolocation",
    "location"
  ],
  "peerDependencies": {
    "react": "*",
    "react-native": "*"
  },
  "dependencies": {},
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@babel/runtime": "^7.12.5",
    "@react-native-community/eslint-config": "^2.0.0",
    "@semantic-release/git": "^7.0.16",
    "babel-jest": "^26.6.3",
    "babel-plugin-module-resolver": "^3.2.0",
    "eslint": "^5.16.0",
    "eslint-config-prettier": "^6.0.0",
    "eslint-plugin-prettier": "3.0.1",
    "flow-bin": "^0.170.0",
    "jest": "^26.6.3",
    "metro-react-native-babel-preset": "0.67.0",
    "prettier": "^2.6.2",
    "react": "17.0.2",
    "react-native": "0.68.2",
    "react-native-builder-bob": "^0.18.2",
    "react-test-renderer": "17.0.2",
    "semantic-release": "^15.13.21"
  },
  "jest": {
    "preset": "react-native",
    "setupFilesAfterEnv": [
      "<rootDir>/jest.setup.js"
    ]
  },
  "react-native-builder-bob": {
    "source": "js",
    "output": "lib",
    "targets": [
      [
        "commonjs",
        {
          "flow": true
        }
      ],
      "module"
    ]
  }
}
